table netdev nftlb {
	map proto-services-lo {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 80 : goto lb01 }
	}

	map static-sessions-lb01 {
		type ipv4_addr : ether_addr
	}

	map persist-lb01 {
		type ipv4_addr : ether_addr
		size 65535
		timeout 1m
	}

	chain ingress-lo {
		type filter hook ingress device "lo" priority 100; policy accept;
		ip protocol . ip daddr . th dport vmap @proto-services-lo
	}

	chain lb01 {
		ether daddr set ip saddr map @static-sessions-lb01 ether saddr set 00:00:00:00:00:00 fwd to "lo"
		ether saddr set 0f:0f:0f:0f:0f:0f ether daddr set jhash ip saddr mod 1 map { 0 : 01:01:01:01:01:01 } update @persist-lb01 { ip saddr : ether daddr } fwd to "lo"
	}
}
