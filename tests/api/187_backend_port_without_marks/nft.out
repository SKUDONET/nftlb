table ip nftlb {
	map nat-port-services {
		type inet_proto . inet_service : verdict
		elements = { tcp . 8080 : goto nat-newfarm }
	}

	map port-services-back-m {
		type mark : ipv4_addr
	}

	map filter-port-services {
		type inet_proto . inet_service : verdict
		elements = { tcp . 8080 : goto filter-newfarm }
	}

	chain prerouting {
		type nat hook prerouting priority filter; policy accept;
		ip protocol . th dport vmap @nat-port-services
	}

	chain postrouting {
		type nat hook postrouting priority srcnat; policy accept;
		ct mark 0x80000000/1 masquerade
		snat to ct mark map @port-services-back-m
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark set ct mark
		ip protocol . th dport vmap @filter-port-services
	}

	chain filter-newfarm {
		ct state new ct mark 0x00000000 ct mark set 0x80000000
	}

	chain nat-newfarm {
		dnat ip addr . port to numgen inc mod 2 map { 0 : 192.168.100.254 . 80, 1 : 192.168.101.254 . 32 }
	}
}
