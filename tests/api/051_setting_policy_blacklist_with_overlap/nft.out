table ip nftlb {
	map nat-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 192.168.105.189 . 9443 : goto nat-newfarm,
			     tcp . 192.168.105.189 . 9444 : goto nat-newfarm }
	}

	map proto-services-back-m {
		type mark : ipv4_addr
		elements = { 0x00000200 : 192.168.104.22 }
	}

	map proto-services-back {
		type inet_proto . ipv4_addr . inet_service : ipv4_addr
	}

	map filter-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 192.168.105.189 . 9443 : goto filter-newfarm,
			     tcp . 192.168.105.189 . 9444 : goto filter-newfarm }
	}

	map static-sessions-newfarm {
		type ipv4_addr : mark
	}

	map persist-newfarm {
		type ipv4_addr : mark
		size 65535
		timeout 2m
	}

	chain prerouting {
		type nat hook prerouting priority filter; policy accept;
		ip protocol . ip daddr . th dport vmap @nat-proto-services
	}

	chain postrouting {
		type nat hook postrouting priority srcnat; policy accept;
		ct mark 0x80000000/1 masquerade
		snat to ct mark map @proto-services-back-m
		snat to ip protocol . ip daddr . th dport map @proto-services-back
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark set ct mark
		ip protocol . ip daddr . th dport vmap @filter-proto-services
	}

	chain filter-newfarm {
		ct mark set ip saddr map @static-sessions-newfarm accept
		ct state new ct mark set ip saddr map @persist-newfarm
		ct state new ct mark 0x00000000 ct mark set numgen random mod 1 map { 0 : 0x00000200 }
		ct mark != 0x00000000 update @persist-newfarm { ip saddr : ct mark }
	}

	chain nat-newfarm {
		dnat ip addr . port to ct mark map { 0x00000200 : 192.168.104.254 . 80 }
	}
}
table netdev nftlb {
	set blacklist {
		type ipv4_addr
		flags interval
		auto-merge
		elements = { 103.15.28.0/24, 103.28.52.93,
			     103.28.53.138, 103.75.190.11,
			     103.208.220.122, 103.208.220.226,
			     103.234.220.195, 103.234.220.197,
			     103.236.201.88, 103.236.201.110,
			     104.192.0.58, 104.194.228.240,
			     104.196.43.128 }
	}
}
