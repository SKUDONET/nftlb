table netdev nftlb {
	set mylist {
		type ipv4_addr
		flags interval
		auto-merge
		elements = { 75.195.60.82 }
	}
}
table ip nftlb {
	map nat-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 10.150.28.62 . 443 : goto nat-UI-Dev-l4 }
	}

	map proto-services-back-m {
		type mark : ipv4_addr
		elements = { 0x00000208 : 10.150.28.159, 0x00000209 : 10.150.28.159 }
	}

	map proto-services-back {
		type inet_proto . ipv4_addr . inet_service : ipv4_addr
	}

	map filter-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 10.150.28.62 . 443 : goto filter-UI-Dev-l4 }
	}

	chain prerouting {
		type nat hook prerouting priority dstnat; policy accept;
		ip protocol . ip daddr . th dport vmap @nat-proto-services
	}

	chain postrouting {
		type nat hook postrouting priority srcnat; policy accept;
		ct mark 0x80000000/1 masquerade
		snat to ct mark map @proto-services-back-m
		snat to ip protocol . ip daddr . th dport map @proto-services-back
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark set ct mark
		ip protocol . ip daddr . th dport vmap @filter-proto-services
	}

	chain filter-UI-Dev-l4 {
		ct state new ct mark 0x00000000 ct mark set numgen random mod 2 map { 0-1 : 0x00000209 }
	}

	chain nat-UI-Dev-l4 {
		log prefix "IN-UI-Dev-l4 "
		dnat ip addr . port to ct mark map { 0x00000208 : 10.150.28.227 . 449, 0x00000209 : 10.150.28.229 . 449 }
	}
}
