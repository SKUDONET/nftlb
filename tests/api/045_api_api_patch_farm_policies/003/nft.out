table netdev nftlb {
	set aa_black1 {
		type ipv4_addr
		flags interval
		auto-merge
	}

	set aa_black2 {
		type ipv4_addr
		flags interval
		auto-merge
	}

	set aa_white1 {
		type ipv4_addr
		flags interval
		auto-merge
	}

	set aa_white2 {
		type ipv4_addr
		flags interval
		auto-merge
	}

	map proto-services-lo {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 90 : goto newfarm }
	}

	set aa_white1-newfarm-cnt {
		type ipv4_addr
		size 65535
		flags dynamic,timeout
		timeout 2m
	}

	set aa_white2-newfarm-cnt {
		type ipv4_addr
		size 65535
		flags dynamic,timeout
		timeout 2m
	}

	chain ingress-lo {
		type filter hook ingress device "lo" priority 101; policy accept;
		ip protocol . ip daddr . th dport vmap @proto-services-lo
	}

	chain newfarm {
		ip saddr @aa_white1 add @aa_white1-newfarm-cnt { ip saddr } log prefix "[BL,aa_white1,newfarm]" drop
		ip saddr @aa_white2 add @aa_white2-newfarm-cnt { ip saddr } log prefix "[BL,aa_white2,newfarm]" drop
	}
}
table ip nftlb {
	map filter-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 90 : goto filter-newfarm }
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark 0x00000000 meta mark set ct mark
		ip protocol . ip daddr . th dport vmap @filter-proto-services
	}

	chain filter-newfarm {
	}
}
