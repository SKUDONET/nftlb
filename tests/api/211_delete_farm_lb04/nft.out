table ip nftlb {
	map filter-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 10.72.33.55 . 100 : goto filter-lb50,
			     tcp . 10.72.33.55 . 101 : goto filter-lb50 }
	}

	map nat-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 10.72.33.55 . 100 : goto nat-lb50,
			     tcp . 10.72.33.55 . 101 : goto nat-lb50 }
	}

	map proto-services-back-m {
		type mark : ipv4_addr
	}

	map static-sessions-lb50 {
		type ipv4_addr : mark
	}

	map persist-lb50 {
		type ipv4_addr : mark
		size 65535
		timeout 1m
	}

	map proto-services-back {
		type inet_proto . ipv4_addr . inet_service : ipv4_addr
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark set ct mark
		ip protocol . ip daddr . th dport vmap @filter-proto-services
	}

	chain prerouting {
		type nat hook prerouting priority dstnat; policy accept;
		ip protocol . ip daddr . th dport vmap @nat-proto-services
	}

	chain postrouting {
		type nat hook postrouting priority srcnat; policy accept;
		ct mark 0x00000000 ct mark set meta mark
		ct mark 0x80000000/1 masquerade
		snat to ct mark map @proto-services-back-m
		snat to ip protocol . ip daddr . th dport map @proto-services-back
	}

	chain filter-lb50 {
		ct mark set ip saddr map @static-sessions-lb50 accept
		ct state new ct mark set ip saddr map @persist-lb50
		ct state new ct mark 0x00000000 ct mark set jhash ip saddr mod 10 map { 0-4 : 0x00000202, 5-9 : 0x00000203 }
		ct mark != 0x00000000 update @persist-lb50 { ip saddr : ct mark }
	}

	chain nat-lb50 {
		dnat to ct mark map { 0x00000202 : 192.168.0.10, 0x00000203 : 192.168.0.11 }
	}
}
