table ip nftlb {
	map nat-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 80 : goto nat-newfarm }
	}

	map proto-services-back {
		type inet_proto . ipv4_addr . inet_service : ipv4_addr
	}

	map proto-services-back-m {
		type mark : ipv4_addr
	}

	map filter-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 80 : goto filter-newfarm }
	}

	set new-rtlimit-newfarm {
		type ipv4_addr
		size 65535
		flags dynamic
	}

	map static-sessions-newfarm {
		type ipv4_addr : mark
	}

	map persist-newfarm {
		type ipv4_addr : mark
		size 65535
		timeout 1m
	}

	chain prerouting {
		type nat hook prerouting priority filter; policy accept;
		ip protocol . ip daddr . th dport vmap @nat-proto-services
	}

	chain postrouting {
		type nat hook postrouting priority srcnat; policy accept;
		ct mark 0x80000000/1 masquerade
		snat to ip protocol . ip daddr . th dport map @proto-services-back
		snat to ct mark map @proto-services-back-m
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark set ct mark
		ip protocol . ip daddr . th dport vmap @filter-proto-services
	}

	chain filter-newfarm {
		ct state new add @new-rtlimit-newfarm { ip saddr limit rate over 22/second burst 2 packets } log prefix "new-rtlimit-newfarm " drop
		ct mark set ip saddr map @static-sessions-newfarm accept
		ct state new ct mark set ip saddr map @persist-newfarm
		ct state new ct mark 0x00000000 ct mark set numgen inc mod 1 map { 0 : 0x00000200 }
		update @persist-newfarm { ip saddr : ct mark }
	}

	chain nat-newfarm {
		dnat to ct mark map { 0x00000200 : 127.0.0.2 }
	}
}
table netdev nftlb {
	map proto-services-lo {
		type inet_proto . ipv4_addr . inet_service : verdict
	}

	map map-newfarm-back {
		type ipv4_addr : ether_addr
		size 65535
		timeout 1m
	}

	chain ingress-lo {
		type filter hook ingress device "lo" priority 100; policy accept;
		ip protocol . ip saddr . th sport vmap @proto-services-lo
		ip protocol . ip daddr . th dport vmap @proto-services-lo
	}
}
